  "main": "src/backend/index.ts",
  "scripts": {
    "build:frontend": "concurrently 'npm:type-check' 'npm:build-only'",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.frontend.json --composite false",
    "build:backend": "npx tsc --p tsconfig.backend.json ",
    "build": "concurrently 'npm:build:frontend' 'npm:build:backend'",
    "start": "NODE_ENV=production node dist/index.js",
    "dev:frontend": "vite",
    "dev:backend": "concurrently \"'npm:build:backend -- --watch'\" \"nodemon -q dist/index.js\"",
    "dev": "concurrently \"npx vite\" \"'npm:build:backend -- --watch'\" \"nodemon -q dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
