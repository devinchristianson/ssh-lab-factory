    "@types/node": "^20.8.6",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/tsconfig": "^0.4.0",
    "asciinema-player": "github:devinchristianson/asciinema-player#add-prepare",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
